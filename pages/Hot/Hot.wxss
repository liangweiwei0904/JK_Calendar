page {
  background: #f5f5f5;
}
.ad {
  width: 100%;
  height: 125rpx;
}
.topnavFixed {
  position: fixed;
  display: flex;
  width: 100%;
  height: 80rpx;
  top: 0;
}
.topnavFixed .btn {
  margin: 0;
  padding: 0;
  background-color: #dceef3;
  color: #5d5c5d;
  width: 100% !important;
  font-size: 48rpx;
  font-weight: 400;
}
.topnavAbsolute {
  position: absolute;
  display: flex;
  width: 100%;
  height: 80rpx;
}
.topnavAbsolute .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0;
  padding: 0;
  background-color: #dceef3;
  color: #5d5c5d;
  width: 100% !important;
  font-size: 48rpx;
  font-weight: 400;
}
.zhanwei {
  height: 100rpx;
}
.new {
  position: fixed;
  bottom: 50rpx;
  right: 50rpx;
  width: 80rpx;
  height: 80rpx;
}
.new image {
  width: 100%;
  height: 100%;
}
.main {
  background: #fefefe;
  border-radius: 15rpx;
  margin: 0rpx 20rpx 30rpx 20rpx;
}
.main .main_top {
  padding: 10rpx;
  width: 100%;
  height: 100rpx;
  display: flex;
}
.main .main_top .top_avatarUrl {
  flex: 1;
}
.main .main_top .top_avatarUrl image {
  margin: 10rpx;
  border-radius: 50%;
  width: 90rpx;
  height: 90rpx;
}
.main .main_top .top_wrap {
  flex: 6;
}
.main .main_top .top_wrap .top_name {
  font-size: 37rpx;
  font-weight: 500;
  color: #373737;
}
.main .main_top .top_wrap .top_time {
  font-size: 28rpx;
  color: #b7b7b7;
}
.main .main_middle .middle_content {
  margin: 10rpx 20rpx 15rpx 20rpx;
  font-size: 36rpx;
  color: #373737;
}
.main .main_middle .img_wrap {
  display: flex;
  width: 100%;
  flex-wrap: wrap;
}
.main .main_middle .middle_img {
  margin: 10rpx;
  width: 200rpx;
  height: 200rpx;
}
.main .main_middle .middle_img image {
  box-shadow: 1rpx 1rpx 2rpx #dceef3;
  width: 200rpx;
  height: 200rpx;
}
.main .main_bottom {
  color: #a3a3a3;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.main .main_bottom .iconfont {
  flex: 1;
  width: 33%;
  padding: 0rpx 80rpx;
}
